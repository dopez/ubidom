<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ubi.erp.prod1.dao.ComponentCodeSDao">

	<resultMap id="ComponentCodeSMap" type="ComponentCodeS"/>

	<select id="selComponentCodeSL" statementType="CALLABLE">
		 {call SP_PROD1_COMPONENTCODES_SCH1(#{partCode},#{partName},#{o_cursor, mode=OUT, jdbcType=CURSOR, resultMap=ComponentCodeSMap, javaType=ResultSet})}
	</select>


	<select id="selComponentCodeSR" statementType="CALLABLE">
		 {call SP_PROD1_COMPONENTCODES_SCH2(#{partCode},#{o_cursor, mode=OUT, jdbcType=CURSOR, resultMap=ComponentCodeSMap, javaType=ResultSet})}	
	</select>
	
		<insert id="prcsComponentCodeS" statementType="CALLABLE" parameterType="ComponentCodeS">
  		{call SP_PROD1_COMPONENTCODES_SAVE(
  		#{cudKey},#{partCode},#{kind1},#{kind2},#{partName},
  		#{partSpec},#{partUnit},#{rmk},#{sysEmpNo}
  		)}	
  	</insert>
</mapper>