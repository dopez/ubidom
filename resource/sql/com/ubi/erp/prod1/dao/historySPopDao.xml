<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ubi.erp.prod1.dao.HistorySPopDao">

	<resultMap id="HistorySPopMap" type="HistorySPop"/>

	<select id="selEquiCheck" statementType="CALLABLE">
		 {call SP_PROD1_EQUICHECK_SCH(#{o_cursor, mode=OUT, jdbcType=CURSOR, resultMap=HistorySPopMap, javaType=ResultSet})}
	</select>
	
	<insert id="prcsEquiCheck" statementType="CALLABLE" parameterType="HistoryS">
  		{call SP_PROD1_EQUICHECK_SAVE(
  		#{cudKey},#{equiCode},#{checkItem},#{cycleKind},#{cycle},
  		#{finalDate},#{rmk},#{sysEmpNo}
  		)}	
  	</insert>
  	
  	<select id="selPartCodeHistory" statementType="CALLABLE">
		 {call SP_PROD1_PARTCODEHISTORY_SCH(#{o_cursor, mode=OUT, jdbcType=CURSOR, resultMap=HistorySPopMap, javaType=ResultSet})}
	</select>
	
	<insert id="prcsPartCodeHistory" statementType="CALLABLE" parameterType="HistoryS">
  		{call SP_PROD1_PARTCODEHISTORY_SAVE(
  		#{cudKey},#{equiCode},#{partCode},#{partName},#{partSpec},
  		#{cycleKind},#{cycle},#{safeStock},#{insurance},#{sysEmpNo}
  		)}	
  	</insert>
  	
  	<select id="selPartCode" statementType="CALLABLE">
		 {call SP_PROD1_PARTCODE_SCH(#{partName},#{o_cursor, mode=OUT, jdbcType=CURSOR, resultMap=HistorySPopMap, javaType=ResultSet})}	
	</select>
	
</mapper>

