<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ubi.erp.mang.dao.CostSDao">

	<resultMap type="CostS" id="CostSMap"/>

 	<select id="getSrh" statementType="CALLABLE">
		 {call SP_ACC_CODES_SCH(
		 		#{V_COMPID},
		 		#{V_AC_CD1},
		 		#{o_cursor, mode=OUT, jdbcType=CURSOR, resultMap=CostSMap, javaType=ResultSet}
		 	)
		 }
	</select>
 	<select id="prcsSave" statementType="CALLABLE">
		 {call SP_ACC_CODES_SAVE(
		 		#{cudKey},
		 		#{compId},
		 		#{acCd1},
		 		#{acCd2},
		 		#{acCd3},
		 		#{acCd4},
		 		#{acSeq},
		 		#{acCode},
		 		#{acNm1},
		 		#{acNm2},
		 		#{acNm3},
		 		#{acNm4},
		 		#{acName},
		 		#{cAcCode},
		 		#{crDrChk},
		 		#{codeChk},
		 		#{acCodeOld},
		 		#{sysEmpNo}
		 	)
		 }
	</select>
</mapper>
